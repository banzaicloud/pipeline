go_library(
    name = "objectstore",
    srcs = glob(
        ["*.go"],
        exclude = ["*_test.go"],
    ),
    visibility = ["PUBLIC"],
    deps = [
        "//pkg/errors",
        "//third_party/go:cloud.google.com__go__storage",
        "//third_party/go:emperror.dev__errors",
        "//third_party/go:golang.org__x__oauth2",
        "//third_party/go:google.golang.org__api",
    ],
)

go_test(
    name = "test",
    srcs = glob(["*.go"]),
    deps = [
        "//pkg/errors",
        "//third_party/go:cloud.google.com__go__storage",
        "//third_party/go:emperror.dev__emperror",
        "//third_party/go:emperror.dev__errors",
        "//third_party/go:github.com__pkg__errors",
        "//third_party/go:github.com__stretchr__testify",
        "//third_party/go:golang.org__x__oauth2",
        "//third_party/go:google.golang.org__api",
    ],
)

go_test(
    name = "integration_test",
    srcs = glob(["*.go"]),
    flags = "-test.run ^TestIntegration$",
    labels = ["integration"],
    deps = [
        "//pkg/errors",
        "//third_party/go:cloud.google.com__go__storage",
        "//third_party/go:emperror.dev__emperror",
        "//third_party/go:emperror.dev__errors",
        "//third_party/go:github.com__pkg__errors",
        "//third_party/go:github.com__stretchr__testify",
        "//third_party/go:golang.org__x__oauth2",
        "//third_party/go:google.golang.org__api",
    ],
)
